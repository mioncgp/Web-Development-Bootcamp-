<%- include("header") -%>
<body>
  <div class="box" id="heading"><h1><%= listTitle %></h1></div>
  <div class="box">
    <% newListItems.forEach((item, index) => { %>
    <div class="item">
      <input type="checkbox" />
      <p><%= newListItems[index] %></p>
    </div>
    <% }) %>
    <form class="item" action="/" method="POST">
      <input
        type="text"
        name="newItem"
        placeholder="New Item..."
        autocomplete="off"
      />
      <button type="submit" name="button" value="<%= listTitle %>">+</button>
    </form>
  </div>
</body>
<%- include("footer") -%>
